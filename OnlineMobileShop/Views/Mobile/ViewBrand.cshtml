@model IEnumerable<OnlineMobileShop.Entity.Brand>

@{
    ViewBag.Title = "Brand";
    

}

<head>
    <style>
        #table {
            border-spacing: 20px;
            padding: 15px;
        }
    </style>
</head>

<h2>MobileDetails</h2>

<table>
    <tr>
        <td>BrandID</td>
        <td>Brand </td>
      
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.BrandID</td>
            <td>@item.BrandName</td>
        
        </tr>

    }
   
</table>

