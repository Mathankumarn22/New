@model IEnumerable<OnlineMobileShop.Entity.Mobile>

@{
    ViewBag.Title = "MobileDetails";
    Layout = "~/Views/Shared/_Admin.cshtml";

}

<head>
    <style>
        #table {
            border-spacing: 20px;
            padding: 15px;
        }
    </style>
</head>
<table>
    <tr>
        
        <td>Model </td>
        <td>Battery </td>
        <td>RAM</td>
        <td>ROM</td>
        <td>Price </td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
        
            <td>@item.Model</td>
            <td>@item.Battery</td>
            <td>@item.RAM</td>
            <td>@item.ROM</td>
            <td>@item.Price</td>
        </tr>
    }
    </table>

